{
  "extends": "tomer/tsconfig.json",
  "include": ["**/*"],
  "typedocOptions": {
    "entryPoints": "./src/index.d.ts",
    "entryDocument": "readme.md",
    "excludeInternal": true,
    "exclude": "./test",
    "intentionallyNotExported": [
      "CheckNumericLiteral",
      "Curried",
      "MaybePromiseLike",
      "NonNegative",
      "PositiveInteger",
      "StrictInteger"
    ]
  }
}
