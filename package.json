    "require": "./dist/index.min.cjs",
    "import": "./dist/index.min.js"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.min.cjs",
  "module": "./dist/index.min.js",
  "scripts": {
    "prepare": "tomer install",
    "format": "tomer format",
    "lint": "tomer lint",
    "typecheck": "tomer typecheck",
