    "async"
  ],
  "homepage": "https://github.com/TomerAberbach/lfi",
  "repository": "TomerAberbach/lfi",
  "bugs": {
    "url": "https://github.com/TomerAberbach/lfi/issues"
  },
  "license": "Apache-2.0",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
