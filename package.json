    "prettier": "^3.2.5",
    "svkc": "^0.0.2",
    "tomer": "^3.0.4",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.3"
  }
