    "prettier": "^2.8.8",
    "svkc": "^0.0.2",
    "tomer": "^2.0.2",
    "typedoc": "^0.23.14",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^5.0.4"
  }
